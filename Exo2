class Point (x, y)

    x: int
    y: int

START
    fun getX()
    START
        return x
    END

    fun getY()
    START
        return y
    END

    fonction setX(x : int)
    START
        x = int
    END

    fun setY(y: int)
    START
        y = int
    END

    START
        print  "x : " + str(x)
        print  "y : " + str(y)
    END

END

    main()
        p: Point

    START
        p.setX(1)
        p.setY(2)
        p.toString()

    END
